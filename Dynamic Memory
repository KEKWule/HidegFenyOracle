#include <iostream>
using namespace std;

int main() {
    int x, n;
	//cout << "Enter the number of items:" << "\n";
	cin >>n;
	int *arr = new int(n);
	//cout << "Enter " << n << " items" << endl;
	for (x = 0; x < n; x++) {
		cin >> arr[x];
	}
    

    int max = arr[0];
    for(int i=0; i<n; i++) {
        if(arr[i]>max)
            max = arr[i];
    }
    cout << max;

    return 0;
}
